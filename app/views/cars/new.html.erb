<h1>Cars#new</h1>
<p>Find me in app/views/cars/new.html.erb</p>

<%= simple_form_for [@car] do |f| %>
  <%= f.input :manufacturer %>
  <%= f.input :year %>
  <%= f.input :model %>
  <%= f.input :category %>
  <%= f.input :day_price %>
  <%= f.input :photo, as: :file %>

  <%= f.submit "Submit car", class: "btn btn-primary" %>
<% end %>
