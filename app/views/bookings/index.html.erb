<section>
  <div class="p-3">
    <h1>Here are your bookings</h1>
    <div class="container">
      <%@bookings.each do |booking| %>
      <ul class="bookings">
      <li><%=booking.car.manufacturer%></li>
      <li><%=booking.car.model%></li>
      <li><%=booking.total_price%></li>
      <li><%=booking.start_date%></li>
      <li><%=booking.end_date%></li>
      </ul>
      <%end %>
    </div>
  </div>

  <br>
